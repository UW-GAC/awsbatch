{
  "jobDefinitionName": "topmed_general",
  "type": "container",
  "retryStrategy": {
      "attempts": 2
  },
  "parameters": { "wd" : "/projects/topmed/analysts/batch",
      "dr" : "/projects",
      "rd" : "/usr/local/analysis_pipeline/runRscript.sh",
      "ra" : "-c 1 /usr/local/analysis_pipeline/R/assoc_window.R /projects/topmed/analysts/batch/somefile.cfg",
      "lf" : "runrscript.log",
      "tmo": "15",
      "db" : "0",
      "po" : "0",
      "mt" : "mount -t nfs4 -o vers=4.1 172.31.58.33:/ /projects"
  },
  "containerProperties": {
    "image": "rollinroy/r-332-tm",
    "vcpus": 2,
    "memory": 7000,
    "privileged": true,
    "command": [
        "/usr/local/analysis_pipeline/runRscript.py",
        "--workdir", "Ref::wd",
        "--dataroot", "Ref::dr",
        "--rdriver", "Ref::rd",
        "--rargs", "Ref::ra",
        "--mount", "Ref::mt",
        "--Debug", "Ref::db",
        "--printonly", "Ref::po",
        "--logfile", "Ref::lf",
        "--timeout", "Ref::tmo"
    ],
    "environment": [
      {
        "name": "SGE_TASK_ID",
        "value": "1"
      }
    ]

    }
}
